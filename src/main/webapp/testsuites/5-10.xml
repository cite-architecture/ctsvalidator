<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../rng/GetPassage.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<cts:GetPassage
	xmlns="http://chs.harvard.edu/xmlns/cts"
	xmlns:cts="http://chs.harvard.edu/xmlns/cts">
	<request>
		<requestName>GetPassage</requestName>
		<requestUrn>urn:cts:latinLit:stoa0115.stoa002:1.1.1</requestUrn>
	</request>
	<cts:reply
		xmlns:tei="http://www.tei-c.org/ns/1.0">
		<urn>urn:cts:latinLit:stoa0115.stoa002.stoa01:1.1.1</urn>
		
		<passage><tei:TEI>
			<tei:text>
				<tei:body>
					<tei:div
						n="1"
						type="colloquium">
						<tei:div
							n="1"
							type="topic">
							<p
								n="1"
								xmlns="http://www.tei-c.org/ns/1.0">Non temere docet quidam, ut
								salutemus libenter. Comis enim et blanda salutatio saepe conciliat
								amicitiam, inimicitiam diluit: certe mutuam benevolentiam alit
								augetque. Quidam usque adeo Demeae sunt, ingenioque agresti, ut vix
								salutati resalutent.</p>
						</tei:div>
					</tei:div>
				</tei:body>
			</tei:text>
		</tei:TEI>
	</passage></cts:reply>
</cts:GetPassage>
